{
  "name": "srhinow/contao-page-images-bundle",
  "description": "Set one or more images central per page in Contao CMS. This is a Contao4 -Bundle-Fork from 'Ruudt/contao-page_images'",
  "keywords": ["contao", "pageimage", "image", "backend modul"],
  "type": "contao-module",
  "license": "GPL-3.0+",
  "authors":[
    {
      "name":"Ruud Walraven",
      "role":"Developer"
    },
    {
      "name":"Sven Rhinow",
      "homepage":"https://www.sr-tag.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email": "kservice@sr-tag.de",
    "source":"https://gitlab.com/srhinow/contao-page-images-bundle",
    "issues":"https://gitlab.com/srhinow/contao-page-images-bundle/issues"
  },
  "require": {
    "php": ">=7.0",
    "contao/core-bundle": "~3.5 || ~4.4",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
  },
  "autoload": {
    "psr-4": {
      "Srhinow\\ContaoPageImagesBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Srhinow\\ContaoPageImagesBundle\\ContaoManager\\Plugin"
  }
}
